{
  "name": "SweatMap",
  "description": "SweatMap is a utility that rewrites takes in strings as keys and makes them as small as possible.",
  "version": "0.0.0",
  "author": {
    "name": "Benjamin Solum",
    "url": "http://soluml.com"
  },
  "preferGlobal": true,
  "keywords": [
    "sweat",
    "sweatmap",
    "sweat map",
    "sweatmap javascript",
    "sweatmap.js",
    "minify",
    "obfuscate",
    "optimize",
    "css"
  ],
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git://github.com/soluml/sweatmap.git"
  },
  "scripts": {
    "build-dev": "NODE_ENV=development webpack",
    "build": "NODE_ENV=production webpack",
    "start": "npm run build && npm run build-dev",
    "pretest": "eslint src/** src/**",
    "test": "mocha"
  },
  "dependencies": {
    "lodash.uniq": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.5.3",
    "mocha": "^2.4.5",
    "webpack": "^1.12.14"
  }
}
